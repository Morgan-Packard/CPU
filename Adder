v0x55829ed29a20_0 .var "o_c", 0 0;
v0x55829ed29ae0_0 .var "o_o", 0 0;
E_0x55829ed12130 .event posedge, v0x55829ed29850_0;
    .scope S_0x55829ed13e80;
T_0 ;
    %wait E_0x55829ed12130;
    %load/vec4 v0x55829ed29960_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55829ed29ae0_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55829ed29a20_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55829ed14110_0;
    %load/vec4 v0x55829ed29790_0;
    %xor;
    %assign/vec4 v0x55829ed29ae0_0, 0;
    %load/vec4 v0x55829ed29790_0;
    %load/vec4 v0x55829ed14110_0;
    %and;
    %load/vec4 v0x55829ed29630_0;
    %load/vec4 v0x55829ed296f0_0;
    %and;
    %or;
    %assign/vec4 v0x55829ed29a20_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55829ed001d0;
T_1 ;
    %delay 5000, 0;
    %load/vec4 v0x55829ed29f10_0;
    %inv;
    %store/vec4 v0x55829ed29f10_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x55829ed001d0;
T_2 ;
