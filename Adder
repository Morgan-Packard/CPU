    .port_info 2 /INPUT 1 "i_a";
    .port_info 3 /INPUT 1 "i_b";
    .port_info 4 /INPUT 1 "i_c";
    .port_info 5 /OUTPUT 1 "o_o";
    .port_info 6 /OUTPUT 1 "o_c";
L_0x55829ed2a270 .functor XOR 1, v0x55829ed29c80_0, v0x55829ed29d40_0, C4<0>, C4<0>;
v0x55829ed14110_0 .net "abxor", 0 0, L_0x55829ed2a270;  1 drivers
v0x55829ed29630_0 .net "i_a", 0 0, v0x55829ed29c80_0;  1 drivers
v0x55829ed296f0_0 .net "i_b", 0 0, v0x55829ed29d40_0;  1 drivers
